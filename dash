			ISBLANK(Moved) || ISBLANK(StartCount) || StartCount = 0,
			BLANK(),
			DIVIDE(Moved, StartCount, 0)
		)
		formatString: 0.0%
		lineageTag: 00000005-0005-0005-0005-000000000002

