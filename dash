table _Measures
	lineageTag: a1b2c3d4-e5f6-7890-abcd-ef1234567890


	measure 'Selected Period Start' = 
		SELECTEDVALUE(DimPeriodStart[PeriodStartDate], BLANK())
		formatString: Short Date
		lineageTag: 00000001-0001-0001-0001-000000000001

	measure 'Selected Period End' = 
		SELECTEDVALUE(DimPeriodEnd[PeriodEndDate], BLANK())
		formatString: Short Date
		lineageTag: 00000001-0001-0001-0001-000000000002

	measure 'Period Start Label' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			"Select Period Start",
			FORMAT(StartDate, "dd MMM yyyy")
		)
		lineageTag: 00000001-0001-0001-0001-000000000003

	measure 'Period End Label' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			"Select Period End",
			FORMAT(EndDate, "dd MMM yyyy")
		)
		lineageTag: 00000001-0001-0001-0001-000000000004

	measure 'Days in Period' = 
		VAR StartDate = [Selected Period Start]
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(StartDate) || ISBLANK(EndDate),
			BLANK(),
			DATEDIFF(StartDate, EndDate, DAY)
		)
		formatString: #,##0
		lineageTag: 00000001-0001-0001-0001-000000000005

	measure 'Selection Status' = 
		VAR StartDate = [Selected Period Start]
		VAR EndDate = [Selected Period End]
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(StartDate) && ISBLANK(EndDate), "‚ö†Ô∏è Select both dates",
			ISBLANK(StartDate), "‚ö†Ô∏è Select Period Start",
			ISBLANK(EndDate), "‚ö†Ô∏è Select Period End",
			StartDate = EndDate, "‚ö†Ô∏è Same date selected",
			StartDate > EndDate, "‚ö†Ô∏è Start date is after End date",
			"‚úì Valid"
		)
		lineageTag: 00000001-0001-0001-0001-000000000006

	measure 'Is Valid Selection' = 
		VAR StartDate = [Selected Period Start]
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			NOT(ISBLANK(StartDate)) && 
			NOT(ISBLANK(EndDate)) && 
			StartDate < EndDate,
			1,
			0
		)
		formatString: 0
		lineageTag: 00000001-0001-0001-0001-000000000007

	measure 'Period Description' = 
		VAR StartLabel = [Period Start Label]
		VAR EndLabel = [Period End Label]
		VAR DaysCount = [Days in Period]
		RETURN
		IF(
			[Is Valid Selection] = 1,
			StartLabel & " to " & EndLabel & " (" & FORMAT(DaysCount, "#,##0") & " days)",
			"Invalid Selection"
		)
		lineageTag: 00000001-0001-0001-0001-000000000008



	measure 'Headcount Period Start' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No"
			)
		)
		formatString: #,##0
		lineageTag: 00000002-0002-0002-0002-000000000001

	measure 'Headcount Period End' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No"
			)
		)
		formatString: #,##0
		lineageTag: 00000002-0002-0002-0002-000000000002

	measure 'Average Headcount' = 
		VAR HC_Start = [Headcount Period Start]
		VAR HC_End = [Headcount Period End]
		RETURN
		IF(
			ISBLANK(HC_Start) || ISBLANK(HC_End),
			BLANK(),
			(HC_Start + HC_End) / 2
		)
		formatString: #,##0.0
		lineageTag: 00000002-0002-0002-0002-000000000003

	measure 'Headcount Change' = 
		VAR HC_Start = [Headcount Period Start]
		VAR HC_End = [Headcount Period End]
		RETURN
		IF(
			ISBLANK(HC_Start) || ISBLANK(HC_End),
			BLANK(),
			HC_End - HC_Start
		)
		formatString: +#,##0;-#,##0;0
		lineageTag: 00000002-0002-0002-0002-000000000004

	measure 'Headcount Change Pct' = 
		VAR HC_Start = [Headcount Period Start]
		VAR HC_End = [Headcount Period End]
		RETURN
		IF(
			ISBLANK(HC_Start) || ISBLANK(HC_End) || HC_Start = 0,
			BLANK(),
			DIVIDE(HC_End - HC_Start, HC_Start, 0)
		)
		formatString: +0.0%;-0.0%;0.0%
		lineageTag: 00000002-0002-0002-0002-000000000005

	measure 'Headcount Including Students Start' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate
			)
		)
		formatString: #,##0
		lineageTag: 00000002-0002-0002-0002-000000000006

	measure 'Headcount Including Students End' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate
			)
		)
		formatString: #,##0
		lineageTag: 00000002-0002-0002-0002-000000000007

	measure 'Student Intern Count Start' = 
