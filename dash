	measure 'Probationary Count Start' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				FactEmployeeSnapshot[IsProbationary] = "Yes"
			)
		)
		formatString: #,##0
		lineageTag: 00000007-0007-0007-0007-000000000001

	measure 'Probationary Count End' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				FactEmployeeSnapshot[IsProbationary] = "Yes"
			)
		)
		formatString: #,##0
		lineageTag: 00000007-0007-0007-0007-000000000002

	measure 'Probationary Losses' = 
		VAR StartDate = [Selected Period Start]
		VAR EndDate = [Selected Period End]
		VAR ProbStart = 
			CALCULATETABLE(
				VALUES(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				FactEmployeeSnapshot[IsProbationary] = "Yes"
			)
		VAR EndEmployees = 
			CALCULATETABLE(
				VALUES(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No"
			)
		RETURN
		IF(
			ISBLANK(StartDate) || ISBLANK(EndDate),
			BLANK(),
			COUNTROWS(EXCEPT(ProbStart, EndEmployees))
		)
		formatString: #,##0
		lineageTag: 00000007-0007-0007-0007-000000000003

	measure 'Probationary Attrition Rate' = 
		VAR Losses = [Probationary Losses]
		VAR ProbCount = [Probationary Count Start]
		RETURN
		IF(
			ISBLANK(Losses) || ISBLANK(ProbCount) || ProbCount = 0,
			BLANK(),
			DIVIDE(Losses, ProbCount, 0)
		)
		formatString: 0.0%
		lineageTag: 00000007-0007-0007-0007-000000000004

	measure 'Probationary Retention Rate' = 
		VAR AttritionRate = [Probationary Attrition Rate]
		RETURN
		IF(
			ISBLANK(AttritionRate),
			BLANK(),
			1 - AttritionRate
		)
		formatString: 0.0%
		lineageTag: 00000007-0007-0007-0007-000000000005

	measure 'Probationary Pct of Workforce' = 
		VAR Prob = [Probationary Count Start]
		VAR TotalHC = [Headcount Period Start]
		RETURN
		IF(
			ISBLANK(Prob) || ISBLANK(TotalHC) || TotalHC = 0,
			BLANK(),
			DIVIDE(Prob, TotalHC, 0)
		)
		formatString: 0.0%
		lineageTag: 00000007-0007-0007-0007-000000000006




	measure 'Annualized Attrition Rate' = 
		VAR AttritionRate = [Attrition Rate]
		VAR DaysInPeriod = [Days in Period]
		RETURN
		IF(
			ISBLANK(AttritionRate) || ISBLANK(DaysInPeriod) || DaysInPeriod = 0,
			BLANK(),
			1 - POWER(1 - AttritionRate, 365 / DaysInPeriod)
		)
		formatString: 0.0%
		lineageTag: 00000008-0008-0008-0008-000000000001

	measure 'Annualized Retention Rate' = 
		VAR RetentionRate = [Retention Rate Agency]
		VAR DaysInPeriod = [Days in Period]
		RETURN
		IF(
			ISBLANK(RetentionRate) || ISBLANK(DaysInPeriod) || DaysInPeriod = 0,
			BLANK(),
			POWER(RetentionRate, 365 / DaysInPeriod)
		)
		formatString: 0.0%
		lineageTag: 00000008-0008-0008-0008-000000000002

	measure 'Annualized Turnover Rate' = 
		VAR TurnoverRate = [Turnover Rate]
		VAR DaysInPeriod = [Days in Period]
		RETURN
		IF(
			ISBLANK(TurnoverRate) || ISBLANK(DaysInPeriod) || DaysInPeriod = 0,
			BLANK(),
			TurnoverRate * (365 / DaysInPeriod)
		)
		formatString: 0.0%
		lineageTag: 00000008-0008-0008-0008-000000000003

	measure 'Monthly Attrition Rate' = 
		VAR AttritionRate = [Attrition Rate]
		VAR DaysInPeriod = [Days in Period]
		RETURN
		IF(
			ISBLANK(AttritionRate) || ISBLANK(DaysInPeriod) || DaysInPeriod = 0,
			BLANK(),
			1 - POWER(1 - AttritionRate, 30 / DaysInPeriod)
		)
		formatString: 0.00%
		lineageTag: 00000008-0008-0008-0008-000000000004

	measure 'Quarterly Attrition Rate' = 
		VAR AttritionRate = [Attrition Rate]
		VAR DaysInPeriod = [Days in Period]
		RETURN
		IF(
			ISBLANK(AttritionRate) || ISBLANK(DaysInPeriod) || DaysInPeriod = 0,
			BLANK(),
			1 - POWER(1 - AttritionRate, 91 / DaysInPeriod)
		)
		formatString: 0.0%
		lineageTag: 00000008-0008-0008-0008-000000000005



	measure 'Workforce Stability Index' = 
		VAR Retention = [Retention Rate Agency]
		VAR Attrition = [Attrition Rate]
		VAR InternalMove = [Internal Movement Rate Org]
		RETURN
		IF(
			ISBLANK(Retention) || ISBLANK(Attrition),
			BLANK(),
			(Retention * 0.5) + 
			((1 - Attrition) * 0.3) + 
			((1 - COALESCE(InternalMove, 0)) * 0.2)
		)
		formatString: 0.00
		lineageTag: 00000009-0009-0009-0009-000000000001

	measure 'Retention Risk Score' = 
		VAR Attrition = [Attrition Rate]
		VAR NewHireAttrition = [New Hire Attrition Rate]
		VAR ProbAttrition = [Probationary Attrition Rate]
		RETURN
		IF(
			ISBLANK(Attrition),
			BLANK(),
			(COALESCE(Attrition, 0) * 0.4) + 
			(COALESCE(NewHireAttrition, 0) * 0.35) + 
			(COALESCE(ProbAttrition, 0) * 0.25)
		)
		formatString: 0.00
		lineageTag: 00000009-0009-0009-0009-000000000002

	measure 'Expected Losses Next Period' = 
		VAR CurrentAttrition = [Attrition Rate]
		VAR CurrentHC = [Headcount Period End]
		RETURN
		IF(
			ISBLANK(CurrentAttrition) || ISBLANK(CurrentHC),
			BLANK(),
			ROUND(CurrentHC * CurrentAttrition, 0)
		)
		formatString: #,##0
		lineageTag: 00000009-0009-0009-0009-000000000003

	measure 'Break Even Hires Needed' = 
		VAR ExpectedLosses = [Expected Losses Next Period]
		RETURN
		IF(
			ISBLANK(ExpectedLosses),
			BLANK(),
			ExpectedLosses
		)
		formatString: #,##0
		lineageTag: 00000009-0009-0009-0009-000000000004

	measure 'Years to Replace Workforce' = 
		VAR AnnualizedAttr = [Annualized Attrition Rate]
		RETURN
		IF(
			ISBLANK(AnnualizedAttr) || AnnualizedAttr = 0,
			BLANK(),
			DIVIDE(1, AnnualizedAttr, BLANK())
		)
		formatString: 0.0
		lineageTag: 00000009-0009-0009-0009-000000000005

	measure 'Churn Rate' = 
		VAR Losses = [Losses Total]
		VAR Gains = [Gains Total]
		VAR StartCount = [Headcount Period Start]
		RETURN
		IF(
			ISBLANK(Losses) || ISBLANK(Gains) || ISBLANK(StartCount) || StartCount = 0,
			BLANK(),
			DIVIDE(MIN(Losses, Gains), StartCount, 0)
		)
		formatString: 0.0%
		lineageTag: 00000009-0009-0009-0009-000000000006

	measure 'Growth Through Hiring' = 
		VAR Gains = [Gains Total]
		VAR Losses = [Losses Total]
		RETURN
		IF(
			ISBLANK(Gains) || ISBLANK(Losses),
			BLANK(),
			IF(Gains > Losses, Gains - Losses, 0)
		)
		formatString: #,##0
		lineageTag: 00000009-0009-0009-0009-000000000007

	measure 'Shrinkage Through Attrition' = 
		VAR Gains = [Gains Total]
		VAR Losses = [Losses Total]
		RETURN
		IF(
			ISBLANK(Gains) || ISBLANK(Losses),
			BLANK(),
			IF(Losses > Gains, Losses - Gains, 0)
		)
		formatString: #,##0
		lineageTag: 00000009-0009-0009-0009-000000000008

	measure 'Retention vs Target 90' = 
		VAR Actual = [Retention Rate Agency]
		VAR Target = 0.90
		RETURN
		IF(
			ISBLANK(Actual),
			BLANK(),
			Actual - Target
		)
		formatString: +0.0%;-0.0%;0.0%
		lineageTag: 00000009-0009-0009-0009-000000000009

	measure 'Attrition vs Target 10' = 
		VAR Actual = [Attrition Rate]
		VAR Target = 0.10
		RETURN
		IF(
			ISBLANK(Actual),
			BLANK(),
			Actual - Target
		)
		formatString: +0.0%;-0.0%;0.0%
		lineageTag: 00000009-0009-0009-0009-000000000010



	measure 'Avg Tenure Years Start' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			BLANK(),
			CALCULATE(
				AVERAGEX(
					FactEmployeeSnapshot,
					VAR SCDText = FactEmployeeSnapshot[SCD Civilian]
					VAR SCDDate = IF(ISBLANK(SCDText), BLANK(), DATEVALUE(SCDText))
					RETURN
					IF(ISBLANK(SCDDate), BLANK(), DATEDIFF(SCDDate, StartDate, DAY) / 365.25)
				),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No"
			)
		)
		formatString: 0.0
		lineageTag: 00000010-0010-0010-0010-000000000001

	measure 'Avg Tenure Years End' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			BLANK(),
			CALCULATE(
				AVERAGEX(
					FactEmployeeSnapshot,
					VAR SCDText = FactEmployeeSnapshot[SCD Civilian]
					VAR SCDDate = IF(ISBLANK(SCDText), BLANK(), DATEVALUE(SCDText))
					RETURN
					IF(ISBLANK(SCDDate), BLANK(), DATEDIFF(SCDDate, EndDate, DAY) / 365.25)
				),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No"
			)
		)
		formatString: 0.0
		lineageTag: 00000010-0010-0010-0010-000000000002

	measure 'Tenure Change' = 
		VAR TenureStart = [Avg Tenure Years Start]
		VAR TenureEnd = [Avg Tenure Years End]
		RETURN
		IF(
			ISBLANK(TenureStart) || ISBLANK(TenureEnd),
			BLANK(),
			TenureEnd - TenureStart
		)
		formatString: +0.0;-0.0;0.0
		lineageTag: 00000010-0010-0010-0010-000000000003



	measure 'Supervisor Count Start' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				FactEmployeeSnapshot[Supervisory Status] IN {"1", "2", "4", "5", "6", "7"}
			)
		)
		formatString: #,##0
		lineageTag: 00000011-0011-0011-0011-000000000001

	measure 'Supervisor Count End' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				FactEmployeeSnapshot[Supervisory Status] IN {"1", "2", "4", "5", "6", "7"}
			)
		)
		formatString: #,##0
		lineageTag: 00000011-0011-0011-0011-000000000002

	measure 'Supervisor Losses' = 
		VAR StartDate = [Selected Period Start]
		VAR EndDate = [Selected Period End]
		VAR SupervisorsStart = 
			CALCULATETABLE(
				VALUES(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				FactEmployeeSnapshot[Supervisory Status] IN {"1", "2", "4", "5", "6", "7"}
			)
		VAR EndEmployees = 
			CALCULATETABLE(
				VALUES(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No"
			)
		RETURN
		IF(
			ISBLANK(StartDate) || ISBLANK(EndDate),
			BLANK(),
			COUNTROWS(EXCEPT(SupervisorsStart, EndEmployees))
		)
		formatString: #,##0
		lineageTag: 00000011-0011-0011-0011-000000000003

	measure 'Supervisor Attrition Rate' = 
		VAR Losses = [Supervisor Losses]
		VAR SupCount = [Supervisor Count Start]
		RETURN
		IF(
			ISBLANK(Losses) || ISBLANK(SupCount) || SupCount = 0,
			BLANK(),
			DIVIDE(Losses, SupCount, 0)
		)
		formatString: 0.0%
		lineageTag: 00000011-0011-0011-0011-000000000004

	measure 'Span of Control Start' = 
		VAR Supervisors = [Supervisor Count Start]
		VAR Total = [Headcount Period Start]
		RETURN
		IF(
			ISBLANK(Supervisors) || ISBLANK(Total) || Supervisors = 0,
			BLANK(),
			DIVIDE(Total - Supervisors, Supervisors, 0)
		)
		formatString: 0.0
		lineageTag: 00000011-0011-0011-0011-000000000005

	measure 'Span of Control End' = 
		VAR Supervisors = [Supervisor Count End]
		VAR Total = [Headcount Period End]
		RETURN
		IF(
			ISBLANK(Supervisors) || ISBLANK(Total) || Supervisors = 0,
			BLANK(),
			DIVIDE(Total - Supervisors, Supervisors, 0)
		)
		formatString: 0.0
		lineageTag: 00000011-0011-0011-0011-000000000006




	measure 'Veterans Count Start' = 
		VAR StartDate = [Selected Period Start]
		RETURN
		IF(
			ISBLANK(StartDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = StartDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				NOT(ISBLANK(FactEmployeeSnapshot[Veterans Status])),
				FactEmployeeSnapshot[Veterans Status] <> "N",
				FactEmployeeSnapshot[Veterans Status] <> ""
			)
		)
		formatString: #,##0
		lineageTag: 00000012-0012-0012-0012-000000000001

	measure 'Veterans Count End' = 
		VAR EndDate = [Selected Period End]
		RETURN
		IF(
			ISBLANK(EndDate),
			BLANK(),
			CALCULATE(
				DISTINCTCOUNT(FactEmployeeSnapshot[EmployeeKey]),
				FactEmployeeSnapshot[SnapshotDate] = EndDate,
				FactEmployeeSnapshot[IsStudentIntern] = "No",
				NOT(ISBLANK(FactEmployeeSnapshot[Veterans Status])),
				FactEmployeeSnapshot[Veterans Status] <> "N",
				FactEmployeeSnapshot[Veterans Status] <> ""
			)
		)
		formatString: #,##0
		lineageTag: 00000012-0012-0012-0012-000000000002

	measure 'Veterans Pct Start' = 
		VAR Vets = [Veterans Count Start]
		VAR Total = [Headcount Period Start]
		RETURN
		IF(
			ISBLANK(Vets) || ISBLANK(Total) || Total = 0,
			BLANK(),
			DIVIDE(Vets, Total, 0)
		)
		formatString: 0.0%
		lineageTag: 00000012-0012-0012-0012-000000000003

	measure 'Veterans Pct End' = 
		VAR Vets = [Veterans Count End]
		VAR Total = [Headcount Period End]
		RETURN
		IF(
			ISBLANK(Vets) || ISBLANK(Total) || Total = 0,
			BLANK(),
			DIVIDE(Vets, Total, 0)
		)
		formatString: 0.0%
		lineageTag: 00000012-0012-0012-0012-000000000004


	/// ========================================================
	/// GROUP 13: FORMATTING & CONDITIONAL MEASURES
	/// ========================================================

	measure 'Retention Rate Color' = 
		VAR Rate = [Retention Rate Agency]
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Rate), "#808080",
			Rate >= 0.9, "#00AA00",
			Rate >= 0.8, "#FFAA00",
			"#CC0000"
		)
		lineageTag: 00000013-0013-0013-0013-000000000001

	measure 'Attrition Rate Color' = 
		VAR Rate = [Attrition Rate]
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Rate), "#808080",
			Rate <= 0.1, "#00AA00",
			Rate <= 0.2, "#FFAA00",
			"#CC0000"
		)
		lineageTag: 00000013-0013-0013-0013-000000000002

	measure 'Headcount Trend Arrow' = 
		VAR Change = [Headcount Change]
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Change), "‚Äî",
			Change > 0, "‚ñ≤",
			Change < 0, "‚ñº",
			"‚Äî"
		)
		lineageTag: 00000013-0013-0013-0013-000000000003

	measure 'Retention Trend Arrow' = 
		VAR Rate = [Retention Rate Agency]
		VAR Target = 0.90
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Rate), "‚Äî",
			Rate >= Target, "‚ñ≤",
			Rate >= 0.85, "‚ñ∫",
			"‚ñº"
		)
		lineageTag: 00000013-0013-0013-0013-000000000004

	measure 'Attrition Trend Arrow' = 
		VAR Rate = [Attrition Rate]
		VAR Target = 0.10
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Rate), "‚Äî",
			Rate <= Target, "‚ñº",
			Rate <= 0.15, "‚ñ∫",
			"‚ñ≤"
		)
		lineageTag: 00000013-0013-0013-0013-000000000005

	measure 'Status Badge' = 
		VAR Retention = [Retention Rate Agency]
		VAR Attrition = [Attrition Rate]
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Retention), "‚ö™ No Data",
			Retention >= 0.95 && Attrition <= 0.05, "üü¢ Excellent",
			Retention >= 0.90 && Attrition <= 0.10, "üü¢ Good",
			Retention >= 0.85 && Attrition <= 0.15, "üü° Watch",
			Retention >= 0.80 && Attrition <= 0.20, "üü† Concern",
			"üî¥ Critical"
		)
		lineageTag: 00000013-0013-0013-0013-000000000006

	measure 'Net Change Icon' = 
		VAR Change = [Net Change]
		RETURN
		SWITCH(
			TRUE(),
			ISBLANK(Change), "‚Äî",
			Change > 10, "‚¨ÜÔ∏è Strong Growth",
			Change > 0, "‚ÜóÔ∏è Growth",
			Change = 0, "‚û°Ô∏è Stable",
			Change > -10, "‚ÜòÔ∏è Decline",
			"‚¨áÔ∏è Sharp Decline"
		)
		lineageTag: 00000013-0013-0013-0013-000000000007


	/// ========================================================
	/// GROUP 14: SUMMARY CARDS
	/// ========================================================

	measure 'Executive Summary' = 
		VAR HC_Start = [Headcount Period Start]
		VAR HC_End = [Headcount Period End]
		VAR Retention = [Retention Rate Agency]
		VAR Attrition = [Attrition Rate]
		VAR Losses = [Losses Total]
		VAR Gains = [Gains Total]
		RETURN
		IF(
			[Is Valid Selection] = 0,
			"Please select valid Period Start and Period End dates.",
			"Workforce: " & FORMAT(HC_Start, "#,##0") & " ‚Üí " & FORMAT(HC_End, "#,##0") & 
			" | Retention: " & FORMAT(Retention, "0.0%") & 
			" | Attrition: " & FORMAT(Attrition, "0.0%") & 
			" | Losses: " & FORMAT(Losses, "#,##0") & 
			" | Gains: " & FORMAT(Gains, "#,##0")
		)
		lineageTag: 00000014-0014-0014-0014-000000000001

	measure 'KPI Summary Card' = 
		VAR Retention = [Retention Rate Agency]
		VAR Attrition = [Attrition Rate]
		VAR NewHireAttr = [New Hire Attrition Rate]
		RETURN
		IF(
			[Is Valid Selection] = 0,
			"",
			"Retention: " & FORMAT(Retention, "0.0%") & UNICHAR(10) &
			"Attrition: " & FORMAT(Attrition, "0.0%") & UNICHAR(10) &
			"New Hire Attrition: " & FORMAT(NewHireAttr, "0.0%")
		)
		lineageTag: 00000014-0014-0014-0014-000000000002


	partition '_Measures' = m
		mode: import
		source =
			let
				Source = #table(type table [Helper = text], {})
			in
				Source

	annotation PBI_ResultType = Table
